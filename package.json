{
  "name": "@deps/traverse",
  "version": "1.0.0",
  "description": "A Traverser library for object.",
  "keywords": [
    "traverse",
    "traverser",
    "walker",
    "tree",
    "object",
    "typescript"
  ],
  "homepage": "https://github.com/azu/traverse",
  "bugs": {
    "url": "https://github.com/azu/traverse/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/azu/traverse.git"
  },
  "license": "MIT",
  "author": "azu",
  "files": [
    "bin/",
    "lib/",
    "src/"
  ],
  "main": "lib/traverse.js",
  "types": "lib/traverse.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc -p .",
    "clean": "rimraf lib/",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "prepublish": "npm run --if-present build",
    "test": "mocha \"test/**/*.ts\"",
    "watch": "tsc -p . --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": false,
    "tabWidth": 4
  },
  "devDependencies": {
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.1",
    "cross-env": "^7.0.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "mocha": "^7.0.1",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "ts-node": "^8.6.2",
    "ts-node-test-register": "^8.0.1",
    "typescript": "^3.7.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
